{
  "name": "svelte-headless-dropdown",
  "description": "A headless, accessible, typeahead-enabled dropdown component for Svelte",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "keywords": [
    "svelte",
    "dropdown",
    "headless",
    "select",
    "autocomplete / typeahead",
    "accessible"
  ],
  "main": "dist/svelte-headless-dropdown.umd.js",
  "module": "dist/svelte-headless-dropdown.es.js",
  "types": "dist/types/index.d.ts",
  "svelte": "dist/svelte-headless-dropdown.es.js",
  "exports": {
    ".": {
      "import": "./dist/svelte-headless-dropdown.es.js",
      "require": "./dist/svelte-headless-dropdown.umd.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "vite build && tsc --project tsconfig.app.json",
    "package": "npm run build",
    "prepare": "npm run build",
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json",
    "format": "prettier --write .",
    "lint": "prettier --check ."
  },
  "peerDependencies": {
    "svelte": "^5.38.1"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.1.1",
    "@tsconfig/svelte": "^5.0.4",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "svelte": "^5.38.1",
    "svelte-check": "^4.3.1",
    "typescript": "~5.8.3",
    "vite": "^7.1.2"
  }
}
